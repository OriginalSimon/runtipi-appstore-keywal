{
    "services": [
      {
        "name": "tsdproxy",
        "image": "almeidapaulopt/tsdproxy:latest",
        "isMain": true,
        "internalPort": "8080",
        "volumes": [
          {
            "hostPath": "${APP_DATA_DIR}/data",
            "containerPath": "/data"
          },
          {
            "hostPath": "${APP_DATA_DIR}/config",
            "containerPath": "/config"
          },
          {
            "hostPath": "/var/run/docker.sock",
            "containerPath": "/var/run/docker.sock"
          }
        ]
      }
]
}
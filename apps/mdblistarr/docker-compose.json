{
  "services": [
    {
      "name": "mdblistarr",
      "image": "linaspurinis/mdblistarr:v2-beta",
      "isMain": true,
      "internalPort": "5353",
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/db",
          "containerPath": "/usr/src/db/",
          "readOnly": false
        }
      ],
      "environment": {
        "PORT": "5353"
      }
    }
  ]
}